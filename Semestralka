#include <iostream>
#include <map>
#include <vector>
using namespace std;

class Matrix


{
public:
    
    map <array<int, 2>, int> newmap;
    //newmap[{1,2}]=333;
    //newmap[{1,3}]=333;
    int n=155;
    
    void set()
    {
    newmap[{1,2}]=333;
    newmap[{1,3}]=333;
    }

    void printed()
    {
    map <array<int, 2>, int>:: iterator itern=newmap.begin();
    
    for(int j=0; itern!=newmap.end(); itern++,j++)
    {
    cout<<itern->first[0]<<" "<<itern->first[1]<<" "<<itern->second<<endl;
    }    
    
    }
   
    
};

